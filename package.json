{
  "name": "astroland",
  "type": "module",
  "version": "0.0.1",
  "homepage": "https://joska-p.github.io/astroland",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "rm -rf dist && eslint . --fix --report-unused-disable-directives && prettier --write . ",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --fix --report-unused-disable-directives",
    "format": "prettier --write . ",
    "fix": "prettier --write .  && eslint . --report-unused-disable-directives --fix"
  },
  "imports": {
    "#assets/*": "./src/assets/*",
    "#components/*": "./src/components/*",
    "#widgets/*": "./src/components/widgets/*",
    "#content/*": "./src/content/*",
    "#layouts/*": "./src/layouts/*",
    "#lib/*": "./src/lib/*",
    "#styles/*": "./src/styles/*",
    "#*": "./src/*"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.19.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.10",
    "clsx": "^2.1.1",
    "eslint": "^9.19.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "dependencies": {
    "astro": "^5.1.9"
  }
}
